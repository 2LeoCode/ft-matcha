{
  "include": ["ts"],
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["DOM", "ES2022"],
    "module": "ES2022",
    "outDir": "js",
    "rootDir": "ts",
    "strict": true,
    "alwaysStrict": true,
    "exactOptionalPropertyTypes": true,
    "noImplicitOverride": true,
    "allowSyntheticDefaultImports": true,
    "noUncheckedIndexedAccess": true
  }
}
